# Mailchimp Configuration
# Copy this file to .env and fill in your actual values

# Your Mailchimp API Key (Account > Extras > API keys)
MAILCHIMP_API_KEY=your_api_key_here

# Server prefix from your API key (the part after the dash, e.g., 'us1', 'us2')
MAILCHIMP_SERVER_PREFIX=us12

# Your Audience (List) ID (Audience > Settings > Audience name and defaults)
MAILCHIMP_AUDIENCE_ID=your_audience_id_here

# Server Port
PORT=3000